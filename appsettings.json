{
  "AppOptions": {
    "Port": 5000
  },
  "ScrapeOptions": {
    "User": "{{RYM_USERNAME}}",
    "IntervalMinutes": 60,
    "CheckOnLaunch": true,
    "Cookies": [
      "is_logged_in=1; Expires=Tue, 01-Jan-2030 01:00:00 GMT; Path=/; HttpOnly;"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
